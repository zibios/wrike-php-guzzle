{
    "name": "zibios/wrike-php-guzzle",
    "description": "HTTP Client and exception transformer adapter for Wrike PHP Library, based on Guzzle package",
    "keywords": ["wrike", "wrike.com", "api", "sdk", "library", "guzzle", "client", "REST", "HTTP", "php"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Zbigniew Ślązak"
        }
    ],
    "require": {
        "php": ">=5.5,<7.2",
        "zibios/wrike-php-library": "^2.0",
        "guzzlehttp/guzzle": ">=4.0,<7.0",
      "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.0,<6.0"
    },
    "autoload": {
        "psr-4": {"Zibios\\WrikePhpGuzzle\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": { "Zibios\\WrikePhpGuzzle\\Tests\\": "tests/" }
    }
}
