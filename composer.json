{
  "name": "zack-carlson/wrike-php-guzzle",
  "description": "HTTP Client and exception transformer adapter for Wrike PHP Library, based on Guzzle package",
  "keywords": [
    "wrike",
    "wrike.com",
    "api",
    "sdk",
    "library",
    "guzzle",
    "client",
    "REST",
    "HTTP",
    "php"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Zbigniew Ślązak"
    }
  ],
  "require": {
    "php": ">=7.1",
    "zack-carlson/wrike-php-library": "dev-master",
    "guzzlehttp/guzzle": ">=6.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": ">=6.0"
  },
  "autoload": {
    "psr-4": {
      "Zibios\\WrikePhpGuzzle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zibios\\WrikePhpGuzzle\\Tests\\": "tests/"
    }
  }
}
